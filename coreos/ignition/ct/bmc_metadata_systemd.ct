systemd:
  units:
    - name: bmc_metadata.service
      enabled: true
      contents: |
        [Unit]
        Description=Metadata population for INAP BMC
        Before=systemd-networkd.service
        [Service]
        Type=oneshot 
		RemainAfterExit=yes
		TimeoutStartSec=60
        ExecStart=/usr/bin/bash -c "/var/lib/bmc/bmc_coreos.sh"
        [Install]
        WantedBy=systemd-networkd.service